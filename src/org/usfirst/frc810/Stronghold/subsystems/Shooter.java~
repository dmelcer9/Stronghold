    private final Solenoid pusher = RobotMap.shooterPusher;
    private final CANTalon leftCannon = RobotMap.shooterLeftCannon;
    private final CANTalon rightCannon = RobotMap.shooterRightCannon;

    // END AUTOGENERATED CODE, SOURCE=ROBOTBUILDER ID=DECLARATIONS


    // Put methods for controlling this subsystem
    // here. Call these from Commands.
    public void startRollers() {
    	
    	leftCannon.setSetpoint(1);
    	rightCannon.setSetpoint(-1);
    	rightCannon.enable();
    	leftCannon.enable();
    	
    }
    
    public void stopRollers() {
    	leftCannon.disable();
    	rightCannon.disable();
    	leftCannon.disableControl();
    	rightCannon.disableControl();
    }
    
    public void pushBall() {
    	pusher.set(true);
    	Timer.delay(.5);
    	pusher.set(false);
    }
